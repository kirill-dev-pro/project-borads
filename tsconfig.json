{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js",
    "types": ["vite/client"],
    "paths": {
      "lib/*": ["lib/*"],
      "views/*": ["src/views/*"],
      "components/*": ["components/*"],
    },
    "baseUrl": "./src"
  },
  "include": ["src"],
}
